{% for plugin in plugins %}
import {{ plugin.name }} from "{{ plugin.packageName }}";
{% endfor %}
import { defineConfig } from "tsdown";

export default defineConfig({
  entry: ["./src/index.ts"],
  dts: {
    sourcemap: false,
  },
  plugins: [
    {% for plugin in plugins %}
    {{ plugin.name }}(){{ "," if loop.last !== true }}
    {% endfor %}
  ],
  tsconfig: "{{ buildTsconfig }}",
});

